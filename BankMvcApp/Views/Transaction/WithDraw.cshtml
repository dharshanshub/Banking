@model BankEntity.Transaction

@{
    ViewData["Title"] = "WithDraw";
     Layout = "_Layout1";
}

<h1>WithDraw</h1>
<section id="With">
<h4>Withdraw</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="WithDraw">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ReceiverAccNo" class="control-label"></label>
                <input asp-for="ReceiverAccNo" class="form-control" />
                <span asp-validation-for="ReceiverAccNo" class="text-danger"></span>
            </div>
            
            
            
            
            <div class="form-group">
                <label asp-for="TransactionAmount" class="control-label"></label>
                <input asp-for="TransactionAmount" class="form-control" />
                <span asp-validation-for="TransactionAmount" class="text-danger"></span>
            </div>
           
            <div class="form-group">
                <label asp-for="Description" class="control-label"></label>
                <input asp-for="Description" class="form-control" />
                <span asp-validation-for="Description" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Submit" class="btn btn-primary" />
            </div>
        </form>
        <p>@ViewBag.result</p>
    </div>
</div>

</section>

